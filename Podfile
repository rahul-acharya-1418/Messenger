# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

# Firebase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage', '~> 8.14.0'

# Facebook
pod 'FBSDKLoginKit'

# Google
pod 'GoogleSignIn'

# Mobile Database
pod 'RealmSwift', '~> 10.33.0'

# Spinner
pod 'JGProgressHUD'

# Message
pod 'MessageKit'

# Images Loading/Download
pod 'SDWebImage'

    
end
